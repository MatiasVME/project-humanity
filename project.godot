[application]

config/name="The Zero Day"
run/main_scene="res://scenes/Main.tscn"
config/icon="res://icon.png"

[autoload]

Main="*res://scenes/autoloads/Main.gd"
Enums="*res://scenes/autoloads/Enums.gd"
DataManager="*res://scenes/autoloads/DataManager.tscn"
PlayerManager="*res://scenes/autoloads/PlayerManager.gd"
Factory="*res://scenes/autoloads/Factory.gd"
ShootManager="*res://scenes/autoloads/ShootManager.gd"
SoundManager="*res://scenes/autoloads/SoundManager.tscn"
CursorManager="*res://scenes/autoloads/CursorManager.gd"
RandomNameGenerator="*res://scenes/autoloads/random_name_generator/RandomNameGenerator.gd"
BuildManager="*res://scenes/autoloads/BuildManager.gd"
StructurePanelManager="*res://scenes/autoloads/StructurePanelManager.tscn"
TerrainGenerator="*res://addons/terrain_generator/TerrainGenerator.gd"

[debug]

gdscript/completion/autocomplete_setters_and_getters=true

[display]

window/size/width=420
window/size/height=240
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "PersistenceNode", "rpg_elements", "wakatime" )

