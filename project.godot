[application]

config/name="ProjectHumanity"
run/main_scene="res://scenes/Main.tscn"
config/icon="res://icon.png"

[autoload]

Main="*res://scenes/autoloads/Main.gd"
DataManager="*res://scenes/autoloads/DataManager.tscn"
PlayerManager="*res://scenes/autoloads/PlayerManager.gd"

[display]

window/size/width=360
window/size/height=240
window/size/test_width=720
window/size/test_height=480
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "PersistenceNode", "rpg_elements", "wakatime" )

[importer_defaults]

texture={
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": true,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

